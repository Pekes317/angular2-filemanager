    "node-sass": "^4.9.0",
    "protractor": "~5.3.2",
    "renamer": "0.6.1",
    "rxjs-es": "^5.0.0-beta.12",
    "ts-node": "~6.0.5",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.1",
    "tslint": "~5.10.0",
    "typescript": "^2.8.3",
    "vinyl-fs": "^3.0.3",
    "webpack-cli": "^2.1.4"
  }
}
