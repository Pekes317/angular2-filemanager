{
  "name": "@beezleeart/ngx-filemanager",
  "version": "1.3.8",
  "license": "MIT",
  "angular-cli": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/qjon/angular2-filemanager.git"
  },
  "keywords": [
    "filemanager",
    "file",
    "manager"
  ],
  "private": false,
  "author": "Rafal Ignaszewski <rafal@ignaszewski.pl> http://ignaszewski.pl",
  "scripts": {
    "ng": "ng",
    "start": "ng serve withoutBackend",
    "start:backend": "ng serve withBackend --proxy-config proxy.config.example.json",
    "backend": "node demo/backend/index.js",
    "build": "ng build",
    "build:app": "cpr dist/ngx-filemanager src/@beezaleeart/lib && ng build --app packages && rimraf src/@beezleeart",
    "build:lib": "rimraf dist && ng build ngx-filemanager && npm run readme",
    "build:prod": "rimraf dist && ng build ngx-filemanager --prod && npm run readme",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "readme": "copyfiles ./package.README.md ./dist/ngx-filemanager/ && cd dist/ngx-filemanager && move package.README.md README.md",
    "publish": "npm publish ./dist/ngx-filemanager"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@beezleeart/ngx-tree": "^2.3.11",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "angular-confirmation-popover": "4.1.0",
    "angular2-notifications": "1.0.2",
    "angular2-uuid": "1.1.1",
    "core-js": "2.5.6",
    "hammerjs": "^2.0.8",
    "ng2-dnd": "^5.0.2",
    "ng2-file-upload": "1.3.0",
    "ng2-translate": "^5.0.0",
    "ngx-image-cropper": "^0.2.7",
    "ngx-img-cropper": "0.10.4",
    "rxjs": "^6.2.0",
    "rxjs-compat": "6.2.0",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/cli": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.1.2",
    "codelyzer": "~4.3.0",
    "copyfiles": "^2.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "easyimage": "^3.1.0",
    "file-loader": "^1.1.11",
    "formidable": "^1.2.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "image-size": "^0.6.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-diff-reporter": "^1.2.0",
    "karma-jasmine-html-reporter": "^1.1.0",
    "ng-packagr": "^3.0.0-rc.4",
